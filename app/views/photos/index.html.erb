<h2>写真一覧画面</h2> <!--タイトル変える-->
<div class="add-photo">
  <% if @album_author == current_user %>
    <%= link_to '写真追加', new_album_photo_path %>
  <% end %>
</div>
<div class="photos">
  <% @photos.each do |photo| %>
    <div>
      <%= link_to album_photo_path(@album.id,photo) do %>
        <%= attachment_image_tag photo, :image, :fill, 150, 150, format: 'jpeg' %>
      <% end %>
    </div>
  <% end %>
</div>